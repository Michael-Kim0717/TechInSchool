        <title> Contact Us </title>
    </head>
    <body>
        <main>
            <div class="container" id="contact-form">
                {{!-- Form --}}
                <form method="POST" action="send">
                    <p id="contactHeader"> Feel free to send us a message! :) </p>
                    <div class="row">
                        <h4> Name : </h4>
                        <input type="text" id="nameField">
                        <h4> Email : </h4>
                        <input type="email" id="emailField">
                        <h4> Subject : </h4>
                        <input type="text" id="subjectField">
                        <h4> Message : </h4>
                        <textarea id="messageField"> </textarea>
                        <button class="btn btn-success" id="sendEmail"> SEND </button>
                    </div>
                </form>
            </div>
        </main>
        
    </body>

</html>